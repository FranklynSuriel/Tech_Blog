<div class="posts">
    {{#each postData}}
    <div class="card">
        <div class="card-body">
            <h2>
                <a href="/post{{id}}">{{title}}</a>
            </h2>
            <div>
                Posted by {{user.username}} on {{format_date createdAt}}
            </div>
            <div class="cardBody">
                {{content}}
            </div>
        </div>
    </div>
    {{/each}}
</div>
